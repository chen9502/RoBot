<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.biyesheji.android.robot">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity"/>
        <activity android:name=".ui.BaseActivity" />
        <activity android:name=".ui.WelcomeActivity" />
        <activity android:name=".ui.OneKindActivity" />
        <activity android:name=".ui.TwoKindActivity" />
        <activity android:name=".ui.ThreeKindActivity" />
        <activity android:name=".ui.FourKindActivity" />
        <activity android:name=".ui.FiveKindActivity"></activity>
    </application>

</manifest>